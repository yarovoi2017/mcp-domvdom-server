{
  "version": 2,
  "name": "mcp-domvdom-server",
  "builds": [
    {
      "src": "server-smithery.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server-smithery.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MCP_SERVER_PORT": "3000",
    "MCP_API_KEY": "test",
    "DOMAIN": "domvdom.com"
  }
} 